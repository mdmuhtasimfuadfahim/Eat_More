
    <!-- home section start -->
    <section class="home" id="home">
        <div class="swiper-container home-slider">
            <div class="swiper-wrapper wrapper">
                <% menus.forEach(function(menus) { %>
                <% if(menus.typeName === "home-dish") { %>
                <!-- single slider start -->
                <div class="swiper-slide slide">
                    <div class="content">
                        <span>our special dish</span>
                        <h3><%= menus.foodName %></h3>
                        <p><%= menus.description %></p>
                        <h4> ৳ <%= menus.price %></h4>
                        <button type="button" data-food="<%= JSON.stringify(menus) %>" class="add-to-cart btn">Order Now</a>
                    </div>
                    <div class="image">
                        <img src="/img/<%= menus.image %>" alt="">
                    </div>
                </div>
                <!-- single slider end -->
                <% } %>
                <% }) %>
            </div>
            <div class="swiper-pagination"></div>
        </div>
    </section>
    <!-- home section end -->

